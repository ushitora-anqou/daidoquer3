{
  "name": "daidoquer3",
  "version": "1.0.0",
  "description": "Yet another Discord bot for speaking text messages aloud and playing music.",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node -r tsconfig-paths/register dist/main",
    "lint-check": "prettier --check src",
    "lint": "prettier --write src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "make -C vendor/yt-dlp/ yt-dlp"
  },
  "repository": "git+https://github.com/ushitora-anqou/daidoquer3.git",
  "keywords": [
    "daidoquer3"
  ],
  "author": "ushitora-anqou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ushitora-anqou/daidoquer3/issues"
  },
  "homepage": "https://github.com/ushitora-anqou/daidoquer3#readme",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@discordjs/voice": "^0.7.2",
    "bufferutil": "^4.0.3",
    "discord-api-types": "^0.23.1",
    "discord.js": "^13.3.1",
    "erlpack": "discord/erlpack",
    "ffmpeg-static": "^4.4.0",
    "niconico-thumbinfo": "^2.0.0",
    "sodium": "^3.0.2",
    "soundcloud.ts": "^0.4.5",
    "utf-8-validate": "^5.0.5",
    "ytdl-core": "^4.9.1",
    "ytpl": "^2.2.3",
    "zlib-sync": "^0.1.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  }
